{
  "name": "Image Search Options - MV3",
  "description": "Image Search Options for Chrome provides a set of highly customizable reverse image search context menu options.",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "event-page.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "iso_16.png",
    "48": "iso_48.png",
    "128": "iso_128.png"
  },
  "manifest_version": 3
}